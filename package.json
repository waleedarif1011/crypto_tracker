{
  "name": "crypto-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up --build",
    "dev:docker:down": "docker-compose -f docker-compose.dev.yml down",
    "dev:docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "build": "vite build",
    "build:prod": "node scripts/build-production.js",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:docker": "docker-compose up --build",
    "build:docker:down": "docker-compose down",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "validate-env": "node scripts/validate-environment.js",
    "validate-env:dev": "node scripts/validate-environment.js development",
    "validate-env:staging": "node scripts/validate-environment.js staging",
    "validate-env:prod": "node scripts/validate-environment.js production",
    "deploy:vercel": "vercel --prod",
    "deploy:vercel:preview": "vercel",
    "vercel:env": "vercel env ls"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  }
}
